<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import tasks.Timer?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity"
            minHeight="-Infinity" minWidth="-Infinity"
            prefHeight="800.0" prefWidth="1280.0"
            xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="controller.GUIControll">
    <padding>
        <Insets top="10" right="5" bottom="10" left="5"/>
    </padding>
    <top>
        <HBox prefHeight="32.0" prefWidth="600.0" spacing="10.0" BorderPane.alignment="CENTER">
            <children>
                <Button mnemonicParsing="false" text="File" fx:id="buttonFile"/>
                <Button mnemonicParsing="false" text="Load" fx:id="buttonLoad"/>
                <Button mnemonicParsing="false" text="Back" fx:id="buttonBack">
                    <HBox.margin>
                        <Insets left="50.0"/>
                    </HBox.margin>
                </Button>
                <Button mnemonicParsing="false" text="Test" fx:id="buttonTest"/>
                <Button mnemonicParsing="false" text="Cycle" fx:id="buttonCycle"/>
                <Button mnemonicParsing="false" text="Help" fx:id="buttonHelp">
                    <HBox.margin>
                        <Insets left="870.0"/>
                    </HBox.margin>
                </Button>
            </children>
            <opaqueInsets>
                <Insets/>
            </opaqueInsets>
        </HBox>
    </top>
    <center>
        <SplitPane dividerPositions="0.783289817232376" orientation="VERTICAL" prefHeight="735.0" prefWidth="960.0">
            <items>
                <TabPane fx:id="tabPane" prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE"
                         BorderPane.alignment="CENTER">
                </TabPane>
                <TextArea prefHeight="26.0" prefWidth="958.0" fx:id="textConsole"/>
            </items>
        </SplitPane>
    </center>
    <right>
        <SplitPane dividerPositions="0.5" orientation="VERTICAL" prefHeight="366.0" prefWidth="160.0"
                   BorderPane.alignment="CENTER">
            <items>
                <ListView fx:id="listView" prefHeight="200.0" prefWidth="200.0"/>
                <ScrollPane prefHeight="200.0" prefWidth="200.0">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="200.0"/>
                    </content>
                </ScrollPane>
            </items>
        </SplitPane>
    </right>
    <left>
        <SplitPane dividerPositions="0.27046997389033943, 0.783289817232376" orientation="VERTICAL" prefHeight="100.0"
                   prefWidth="160.0" BorderPane.alignment="CENTER">
            <items>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="4.0" prefWidth="158.0"/>
                <TreeView prefHeight="478.0" prefWidth="158.0"/>
                <AnchorPane prefHeight="100.0" prefWidth="200.0">
                    <HBox prefHeight="50.0" prefWidth="150.0" Alignment="CENTER">
                        <Label fx:id="timerLabel" text="Timer:" visible="false"/>
                    </HBox>
                    <HBox prefHeight="100.0" prefWidth="150.0" Alignment="CENTER">
                        <Timer fx:id="timer" visible="false"/>
                    </HBox>
                    <HBox prefHeight="150.0" prefWidth="150.0" Alignment="CENTER">
                        <Label fx:id="maxTimer" visible="false"/>
                    </HBox>
                </AnchorPane>
            </items>
        </SplitPane>
    </left>
</BorderPane>
